author: Mark Fiers
backend: gnumake
commands:
- repmask
- clean
creation_date: Wed Nov 10 07:56:48 2010
description: Run a default repeatmask on the input sequences
filesets:
  OUTPUT_FILESET_ID:
    category: output
    dir: ./OUTPUT_FILETYPE
    extension: OUTPUT_FILETYPE
    source: INPUT_FILESET_ID
    type: map
  input:
    category: input
    extension: fasta
    help: Input files for repmask
    optional: false
    type: input
  output:
    category: output
    dir: .
    extension: masked
    source: input
    type: map
gnumake_makefile: /tmp/xx/repeatmasker.mk

    optional: true
    type: integer
  quick:
    allowed:
    - T
    - F
    category: ''
    default: F
    help: Quick job
    optional: true
    type: set
  simple:
    allowed:
    - T
    - F
    category: ''
    default: F
    help: Mask *only* low complex/simple repeats, not interspersed repeats (Repeatmasker
      -(no)int parameter)
    optional: true
    type: set
  species:
    allowed: []
    category: ''
    default: ''
    help: Repeatmasker species
    optional: true
    type: string
  title:
    allowed: []
    category: system
    default: ''
    help: A name for this job
    optional: false
    type: string
